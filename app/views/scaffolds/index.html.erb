<h1>Listing scaffolds</h1>

<table>
  <tr>
    <th>Form</th>
    <th>Textbox</th>
    <th>Textarea</th>
    <th>Submit</th>
    <th>Dropdown</th>
    <th>Value</th>
    <th>Radio</th>
    <th>Check</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @scaffolds.each do |scaffold| %>
  <tr>
    <td><%= scaffold.form %></td>
    <td><%= scaffold.textbox %></td>
    <td><%= scaffold.textarea %></td>
    <td><%= scaffold.submit %></td>
    <td><%= scaffold.dropdown %></td>
    <td><%= scaffold.value %></td>
    <td><%= scaffold.radio %></td>
    <td><%= scaffold.check %></td>
    <td><%= link_to 'Show', scaffold %></td>
    <td><%= link_to 'Edit', edit_scaffold_path(scaffold) %></td>
    <td><%= link_to 'Destroy', scaffold, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Scaffold', new_scaffold_path %>
